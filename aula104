// APREDENDO A USAR AS NOVAS FUNCOES STRINGS 

const nome = new String("Aula Sabando Domingo Quatro")

console.log(nome.startsWith("Aula"))
console.log(nome.endsWith("Quatrp"))
console.log(nome.includes("om"))
console.log(nome.repeat("2"))
console.log(nome.valueOf())

console.log(nome.charCodeAt(0))
console.log(nome.charCodeAt(1))
console.log(nome.charCodeAt(2))
console.log(nome.charCodeAt(3))
console.log(nome.charCodeAt(4))
console.log(nome.charCodeAt(5))

console.log(String.fromCodePoint(108,97,32,83,10))

let nome_ts = "EULER"
console.log(`Nome: ${nome_ts}`)